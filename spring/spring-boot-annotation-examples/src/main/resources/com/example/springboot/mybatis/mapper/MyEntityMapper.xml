<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.springboot.mybatis.mapper.MyEntityMapper">

  <select id="find" resultType="com.example.springboot.entity.MyEntity">
    select id, name from my_entity where id = #{id}
  </select>

  <insert id="insert">
    insert into my_entity(id, name) values (#{id}, #{name})
  </insert>
</mapper>